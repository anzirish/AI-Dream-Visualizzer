# Server Configuration
NODE_ENV=development
PORT=5000

# Database
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/dreamjournal?retryWrites=true&w=majority

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-here
JWT_EXPIRE=7d

# Client Configuration
CLIENT_URL=http://localhost:5173

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# OpenRouter API (for AI story generation)
OPENROUTER_API_KEY=your-openrouter-api-key-here

# Stable Diffusion API (for image generation)
STABLE_DIFFUSION_API_KEY=your-stable-diffusion-api-key-here
